  "paths" : {
    "/servers" : {
      "get" : {
        "tags" : [ "Hardware Monitoring" ],
        "summary" : "Get all the servers being monitored",
        "description" : "",
        "operationId" : "getAll",
        "produces" : [ "application/xml", "application/json" ],
        "parameters" : [ {
          "name" : "listDetailed",
          "in" : "query",
          "description" : "When set to true, a hardwareReportResponse will be included for each server.<br>If hardwareReportResponse.status equals SUCCESS, a detailed hardwareReport for the server will also be included.<br>If hardwareReportResponse.status equals ERROR, then hardwareReportResponse.error will contain a reason.<br>hardwareReportResponse.status will equal INITIALIZING for a brief period after system startup.",
          "required" : false,
          "type" : "boolean"
        } ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "$ref" : "#/definitions/ServerList"
            }
          }
        },
        "x-pmlPermissions" : [ "hpsp_hwmonitor:write", "hpsp_hwmonitor:read" ]
      }
    },
    "/servers/{serverName}" : {
      "get" : {
        "tags" : [ "Hardware Monitoring" ],
        "summary" : "Get a particular server being monitored",
        "description" : "\nA hardwareReportResponse will be included.<br>If hardwareReportResponse.status equals SUCCESS, a detailed hardwareReport for the server will also be included.<br>If hardwareReportResponse.status equals ERROR, then hardwareReportResponse.error will contain a reason.<br>hardwareReportResponse.status will equal INITIALIZING for a brief period after system startup.",
        "operationId" : "getByName",
        "produces" : [ "application/xml", "application/json" ],
        "parameters" : [ {
          "name" : "serverName",
          "in" : "path",
          "description" : "Name of the server",
          "required" : true,
          "type" : "string"
        } ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "$ref" : "#/definitions/ServerModel"
            }
          },
          "204" : {
            "description" : "no server of that name is being monitored"
          }
        },
        "x-pmlPermissions" : [ "hpsp_hwmonitor:write", "hpsp_hwmonitor:read" ]
      }
    },
    "/servers/{serverName}/components" : {
      "get" : {
        "tags" : [ "Hardware Monitoring" ],
        "summary" : "Get all components of a particular type from a server",
        "description" : "",
        "operationId" : "getServerComponents",
        "produces" : [ "application/xml", "application/json" ],
        "parameters" : [ {
          "name" : "serverName",
          "in" : "path",
          "description" : "Server on which to query components",
          "required" : true,
          "type" : "string"
        }, {
          "name" : "type",
          "in" : "query",
          "description" : "Type of component. Supported types are server and fileSystem. You may request multiple component types at once, e.g. type=fileSystem&type=server",
          "required" : true,
          "type" : "array",
          "items" : {
            "type" : "string"
          },
          "collectionFormat" : "multi"
        } ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "$ref" : "#/definitions/ComponentReportResponse"
            }
          },
          "400" : {
            "description" : "invalid input"
          }
        },
        "x-pmlPermissions" : [ "hpsp_hwmonitor:write", "hpsp_hwmonitor:read" ]
      }
    },
    "/servers/{serverName}/components/{componentType}/{property}" : {
      "put" : {
        "tags" : [ "Hardware Monitoring" ],
        "summary" : "Perform an operation such as turning on a UID beacon on a server component",
        "description" : "",
        "operationId" : "performServerComponentOp",
        "produces" : [ "application/xml", "application/json" ],
        "parameters" : [ {
          "name" : "serverName",
          "in" : "path",
          "description" : "Name of the server",
          "required" : true,
          "type" : "string"
        }, {
          "name" : "componentType",
          "in" : "path",
          "description" : "Component type of the component(s) on which to operate. Supported types are server, drive, pool, and volume.",
          "required" : true,
          "type" : "string"
        }, {
          "name" : "property",
          "in" : "path",
          "description" : "Component property to modify. Currently the only supported property is locateBeacon.",
          "required" : true,
          "type" : "string"
        }, {
          "name" : "uuidList",
          "in" : "query",
          "description" : "A uuid or comma separated list of uuids on which to operate. Not required for component type server.",
          "required" : true,
          "type" : "string"
        }, {
          "name" : "operation",
          "in" : "query",
          "description" : "The operation to perform; on or off",
          "required" : true,
          "type" : "string"
        } ],
        "responses" : {
          "202" : {
            "description" : "request accepted"
          },
          "400" : {
            "description" : "invalid input"
          }
        },
        "x-pmlPermissions" : [ "hpsp_hwmonitor:write" ]
      }
    },
